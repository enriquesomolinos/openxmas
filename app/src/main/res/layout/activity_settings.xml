<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="factoriaetsia.com.openxmas.SettingActivity"
    android:rowCount="1"
    android:columnCount="5"
    android:orientation="vertical">

    <TableLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/tableLayout1" android:background="@drawable/tablerow" android:layout_marginLeft="20dp" android:layout_marginRight="20dip"
        android:layout_row="0"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp">
        <TextView android:id="@+id/textView1" android:layout_height="wrap_content" android:text="Salario :" android:layout_width="wrap_content" android:textColor="#FFFFFF" android:textStyle="bold" android:gravity="center_horizontal" android:layout_marginTop="10dip"></TextView>
        <TextView android:layout_gravity="center_vertical|center_horizontal" android:id="@+id/textsalario" android:layout_height="wrap_content" android:text="TextView" android:layout_width="wrap_content" android:gravity="center_horizontal"></TextView>
        <SeekBar android:layout_gravity="center_horizontal" android:layout_height="wrap_content" android:layout_marginLeft="20dip" android:id="@+id/seekSalario" android:layout_width="fill_parent" android:layout_marginRight="20dip" android:paddingBottom="10sp"></SeekBar>
    </TableLayout>

    <TableLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/tableLayout2" android:background="@drawable/tablerow" android:layout_marginLeft="20dp" android:layout_marginRight="20dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dip">
        <TextView android:id="@+id/textView2" android:layout_height="wrap_content" android:text="Ahorros :" android:layout_width="wrap_content" android:textColor="#FFFFFF" android:textStyle="bold" android:gravity="center_horizontal" android:layout_marginTop="10dip"></TextView>
        <TextView android:layout_gravity="center_vertical|center_horizontal" android:id="@+id/textAhorros" android:layout_height="wrap_content" android:text="TextView" android:layout_width="wrap_content" android:gravity="center_horizontal"></TextView>
        <SeekBar android:layout_gravity="center_horizontal" android:layout_height="wrap_content" android:layout_marginLeft="20dip" android:id="@+id/seekAhorros" android:layout_width="fill_parent" android:layout_marginRight="20dip" android:paddingBottom="10sp"></SeekBar>
    </TableLayout>

    <TableLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/tableLayout3" android:background="@drawable/tablerow" android:layout_marginLeft="20dp" android:layout_marginRight="20dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dip">
        <TextView android:id="@+id/textView3" android:layout_height="wrap_content" android:text="Meses en casa :" android:layout_width="wrap_content" android:textColor="#FFFFFF" android:textStyle="bold" android:gravity="center_horizontal" android:layout_marginTop="10dip"></TextView>
        <TextView android:layout_gravity="center_vertical|center_horizontal" android:id="@+id/textTiempoCasa" android:layout_height="wrap_content" android:text="TextView" android:layout_width="wrap_content" android:gravity="center_horizontal"></TextView>
        <SeekBar android:layout_gravity="center_horizontal" android:layout_height="wrap_content" android:layout_marginLeft="20dip" android:id="@+id/seekMesesCasa" android:layout_width="fill_parent" android:layout_marginRight="20dip" android:paddingBottom="10sp"></SeekBar>
    </TableLayout>

    <TableLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/tableLayout4" android:background="@drawable/tablerow" android:layout_marginLeft="20dp" android:layout_marginRight="20dip"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp">
        <TextView android:id="@+id/textView4" android:layout_height="wrap_content" android:text="Meses en Trabajo :" android:layout_width="wrap_content" android:textColor="#FFFFFF" android:textStyle="bold" android:gravity="center_horizontal" android:layout_marginTop="10dip"></TextView>
        <TextView android:layout_gravity="center_vertical|center_horizontal" android:id="@+id/textTiempoTrabajo" android:layout_height="wrap_content" android:text="TextView" android:layout_width="wrap_content" android:gravity="center_horizontal"></TextView>
        <SeekBar android:layout_gravity="center_horizontal" android:layout_height="wrap_content" android:layout_marginLeft="20dip" android:id="@+id/seekMesesTrabajo" android:layout_width="fill_parent" android:layout_marginRight="20dip" android:paddingBottom="10sp"></SeekBar>
    </TableLayout>

    <Button android:id="@+id/button1" android:layout_height="wrap_content" android:layout_gravity="center_horizontal|center" android:background="@drawable/save" android:layout_width="wrap_content" android:layout_marginTop="0dp" android:gravity="center_vertical"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="0dp"
        android:layout_alignParentTop="true"></Button>

</LinearLayout>
